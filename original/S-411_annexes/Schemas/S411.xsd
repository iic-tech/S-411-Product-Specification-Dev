                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="12">
                <xsd:annotation>
                    <xsd:documentation>1/5 – 2/5 or 1/3 – 2/3</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="20">
                <xsd:annotation>
                    <xsd:documentation>2/5 or 2/3</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="23">
                <xsd:annotation>
                    <xsd:documentation>2/5 – 3/5 or 2/3 – 3/3</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="30">
                <xsd:annotation>
                    <xsd:documentation>3/5 or 3/3</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="34">
                <xsd:annotation>
                    <xsd:documentation>3/5 – 4/5</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="40">
                <xsd:annotation>
                    <xsd:documentation>4/5</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="45">
                <xsd:annotation>
                    <xsd:documentation>4/5 – 5/5</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="50">
                <xsd:annotation>
                    <xsd:documentation>5/5</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="98">
                <xsd:annotation>
                    <xsd:documentation>No Melting</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ia_plgType">
        <xsd:annotation>
            <xsd:documentation>Contamination</xsd:documentation>
            <xsd:documentation>Definition: IA_PLG describes the degree of contamination (aerial
                coverage) in thirds based on the Russian national 3-point scale.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>No or insignificant Contamination</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>&lt;1/3 of area</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="12">
                <xsd:annotation>
                    <xsd:documentation>&lt;2/3 of area</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="20">
                <xsd:annotation>
                    <xsd:documentation>1/3 – 2/3 of area</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="23">
                <xsd:annotation>
                    <xsd:documentation>&gt;1/3 area</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="30">
                <xsd:annotation>
                    <xsd:documentation>&gt;2/3 area</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="98">
                <xsd:annotation>
                    <xsd:documentation>No Contamination</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined / Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ia_hlgType">
        <xsd:annotation>
            <xsd:documentation>Hills Concentration</xsd:documentation>
            <xsd:documentation>Definition: IA_HLG describes the degree of hilling in an ice area
                (usually multi-year ice).</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>Few hills</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>Slight concentration of hills</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="12">
                <xsd:annotation>
                    <xsd:documentation>Slight to moderate concentration of hills</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="20">
                <xsd:annotation>
                    <xsd:documentation>Moderate concentration of hills</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="23">
                <xsd:annotation>
                    <xsd:documentation>Moderate to heavy concentration of hills</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="30">
                <xsd:annotation>
                    <xsd:documentation>Heavy concentration of hills</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="98">
                <xsd:annotation>
                    <xsd:documentation>No hills</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ia_dugType">
        <xsd:annotation>
            <xsd:documentation>Fractures Concentration</xsd:documentation>
            <xsd:documentation>Definition: IA_DUG describes the degree of disunity in an ice
                area.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>Frequency of Cracks and Leads seldom in 10 km on
                        Route</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="20">
                <xsd:annotation>
                    <xsd:documentation>Frequency of Cracks and Leads in 5 – 10 km on
                        Route</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="30">
                <xsd:annotation>
                    <xsd:documentation>Frequency of Cracks and Leads in 3 – 5 km on
                        Route</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="40">
                <xsd:annotation>
                    <xsd:documentation>Frequency of Cracks and Leads in 2 – 3 km on
                        Route</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="50">
                <xsd:annotation>
                    <xsd:documentation>Frequency of Cracks and Leads often in 2 km on
                        Route</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="60">
                <xsd:annotation>
                    <xsd:documentation>Small and Medium Floes – 1/10-3/10; Big Floes – 7/10-10/10
                        Conc.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="70">
                <xsd:annotation>
                    <xsd:documentation>Small and Medium Floes – 4/10-6/10; Big Floes – 4/10-6/10
                        Conc.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="80">
                <xsd:annotation>
                    <xsd:documentation>Small and Medium Floes – 7/10-10/10; Big Floes – 1/10-3/10
                        Conc.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="90">
                <xsd:annotation>
                    <xsd:documentation>Small and Medium Floes Only</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="92">
                <xsd:annotation>
                    <xsd:documentation>Small Floes Only</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="98">
                <xsd:annotation>
                    <xsd:documentation>No Fractures</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ia_bcnType">
        <xsd:annotation>
            <xsd:documentation>Iceberg Concentration</xsd:documentation>
            <xsd:documentation>Definition: IA_BCN specifies the total concentration of icebergs in
                an area.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>&gt;45 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="12">
                <xsd:annotation>
                    <xsd:documentation>&gt;15 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="20">
                <xsd:annotation>
                    <xsd:documentation>15 - 44 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="23">
                <xsd:annotation>
                    <xsd:documentation>10 - 44 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="30">
                <xsd:annotation>
                    <xsd:documentation>10 - 14 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="34">
                <xsd:annotation>
                    <xsd:documentation>7 - 14 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="40">
                <xsd:annotation>
                    <xsd:documentation>7 - 10 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="45">
                <xsd:annotation>
                    <xsd:documentation>5 - 10 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="50">
                <xsd:annotation>
                    <xsd:documentation>5 - 6 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="56">
                <xsd:annotation>
                    <xsd:documentation>3 - 6 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="60">
                <xsd:annotation>
                    <xsd:documentation>3 - 4 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="67">
                <xsd:annotation>
                    <xsd:documentation>1 - 4 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="70">
                <xsd:annotation>
                    <xsd:documentation>1 - 2 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="78">
                <xsd:annotation>
                    <xsd:documentation>0.5 - 2.0 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="80">
                <xsd:annotation>
                    <xsd:documentation>0.5 - 1.0 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="89">
                <xsd:annotation>
                    <xsd:documentation>&lt;1.0 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="90">
                <xsd:annotation>
                    <xsd:documentation>&lt;0.5 nm between bergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="98">
                <xsd:annotation>
                    <xsd:documentation>No Icebergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="ia_bfmType">
        <xsd:annotation>
            <xsd:documentation>Prevailing Iceberg Form</xsd:documentation>
            <xsd:documentation>Definition: IA_BFM specifies the prevailing form of icebergs in an
                area.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>Domed</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>Tabular</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="03">
                <xsd:annotation>
                    <xsd:documentation>Sloping</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="04">
                <xsd:annotation>
                    <xsd:documentation>Pinnacled</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="05">
                <xsd:annotation>
                    <xsd:documentation>Dry-dock</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="06">
                <xsd:annotation>
                    <xsd:documentation>Glacier (irregular)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="07">
                <xsd:annotation>
                    <xsd:documentation>Blocky</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="08">
                <xsd:annotation>
                    <xsd:documentation>Weathered</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icebrsType">
        <xsd:annotation>
            <xsd:documentation>Brash Ice</xsd:documentation>
            <xsd:documentation>Definition: ICEBRS specifies the concentrations of very thick brash
                (“AV”), thick brash (“AK”), medium brash (“AM”) and thin brash ice (“AT”). Very
                Thick Brash Ice &gt;4 m Thick Brash Ice &gt;2 - 4 m Medium Brash Ice 1 – 2 m Thin
                Brash Ice &lt;1 m</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>0/10 – 1/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>1/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="12">
                <xsd:annotation>
                    <xsd:documentation>1/10 – 2/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="20">
                <xsd:annotation>
                    <xsd:documentation>2/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="23">
                <xsd:annotation>
                    <xsd:documentation>2/10 – 3/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="30">
                <xsd:annotation>
                    <xsd:documentation>3/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="34">
                <xsd:annotation>
                    <xsd:documentation>3/10 – 4/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="40">
                <xsd:annotation>
                    <xsd:documentation>4/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="45">
                <xsd:annotation>
                    <xsd:documentation>4/10 – 5/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="50">
                <xsd:annotation>
                    <xsd:documentation>5/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="56">
                <xsd:annotation>
                    <xsd:documentation>5/10 – 6/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="60">
                <xsd:annotation>
                    <xsd:documentation>6/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="67">
                <xsd:annotation>
                    <xsd:documentation>6/10 – 7/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="70">
                <xsd:annotation>
                    <xsd:documentation>7/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="78">
                <xsd:annotation>
                    <xsd:documentation>7/10 – 8/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="80">
                <xsd:annotation>
                    <xsd:documentation>8/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="89">
                <xsd:annotation>
                    <xsd:documentation>8/10 – 9/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="90">
                <xsd:annotation>
                    <xsd:documentation>9/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="91">
                <xsd:annotation>
                    <xsd:documentation>9/10 – 10/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="92">
                <xsd:annotation>
                    <xsd:documentation>10/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="98">
                <xsd:annotation>
                    <xsd:documentation>No Brash ice</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
</xsd:schema>
