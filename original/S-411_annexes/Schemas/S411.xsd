    <xsd:simpleType name="iceflzType">
        <xsd:annotation>
            <xsd:documentation>Floe Sizes describe the predominate forms of ice floe sizes (Fa, Fb
                and Fc) corresponding to the ice Stages of Development Sa, Sb and Sc
                respectively.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>Pancake Ice (30 cm to 3m across)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>Shuga/Small Ice Cake; Brash Ice (&lt;2m
                        across)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="03">
                <xsd:annotation>
                    <xsd:documentation>Ice Cake (&lt;20m across)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="04">
                <xsd:annotation>
                    <xsd:documentation>Small Floe (20 to &lt;100m across)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="05">
                <xsd:annotation>
                    <xsd:documentation>Medium Floe (100 to 500m)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="06">
                <xsd:annotation>
                    <xsd:documentation>Big Floe (500 to &lt;2000m across)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="07">
                <xsd:annotation>
                    <xsd:documentation>Vast Floe (2000 to10000m across)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="08">
                <xsd:annotation>
                    <xsd:documentation>Giant Floe (&gt;10000m across)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="09">
                <xsd:annotation>
                    <xsd:documentation>Fast Ice</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>Growlers, Floebergs or Floebits</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="11">
                <xsd:annotation>
                    <xsd:documentation>Icebergs</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icemltType">
        <xsd:annotation>
            <xsd:documentation>The Stage of Melt describes the stages of the melting ice. PUDDLE -
                An accumulation of water on ice, mainly due to the melting of snow, but in some more
                advanced stages also the melting of ice. THAW HOLE - Vertical holes formed in ice
                when surface puddles melt through to the underlying water. DRIED ICE - Ice surface
                from which water has disappeared after the formation of cracks and thaw holes.
                During the process of drying the surface whitens. ROTTEN ICE - Ice which has become
                honey-combed and is in an advanced state of disintegration. FLOODED ICE - Ice which
                has been flooded and is heavily loaded by water or water and wet snow. FROZEN PUDDLE
                - A puddle which has frozen over.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>Few Puddles</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>Many Puddles</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="03">
                <xsd:annotation>
                    <xsd:documentation>Flooded Ice</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="04">
                <xsd:annotation>
                    <xsd:documentation>Few Thaw Holes</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="05">
                <xsd:annotation>
                    <xsd:documentation>Many Thaw Holes</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="06">
                <xsd:annotation>
                    <xsd:documentation>Dried Ice</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="07">
                <xsd:annotation>
                    <xsd:documentation>Rotten Ice</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="08">
                <xsd:annotation>
                    <xsd:documentation>Few Frozen Puddles</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="09">
                <xsd:annotation>
                    <xsd:documentation>All Puddles Frozen</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>No Melt</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icespcType">
        <xsd:annotation>
            <xsd:documentation>A strip is a long narrow area of floating ice, about 1 kilometre or
                less in width, usually composed of small fragments detached from the main mass of
                ice, and run together under the influence of wind, swell or current. If the area of
                ice becomes more rounded in shape, it is referred to as a patch. ICESPC indicates
                the concentration in tenths within the area of Strips and
                Patches.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="11">
                <xsd:annotation>
                    <xsd:documentation>Strips and Patches (concentrations 1/10)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="12">
                <xsd:annotation>
                    <xsd:documentation>Strips and Patches (concentrations 2/10)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="13">
                <xsd:annotation>
                    <xsd:documentation>Strips and Patches (concentrations 3/10)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="14">
                <xsd:annotation>
                    <xsd:documentation>Strips and Patches (concentrations 4/10)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="15">
                <xsd:annotation>
                    <xsd:documentation>Strips and Patches (concentrations 5/10)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="16">
                <xsd:annotation>
                    <xsd:documentation>Strips and Patches (concentrations 6/10)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="17">
                <xsd:annotation>
                    <xsd:documentation>Strips and Patches (concentrations 7/10)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="18">
                <xsd:annotation>
                    <xsd:documentation>Strips and Patches (concentrations 8/10)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="19">
                <xsd:annotation>
                    <xsd:documentation>Strips and Patches (concentrations 9/10)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="20">
                <xsd:annotation>
                    <xsd:documentation>Strips and Patches (concentrations 10/10)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icelvlType">
        <xsd:annotation>
            <xsd:documentation>ICELVL is an indication as to whether or not the ice has been
                affected by deformation</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>Level (undeformed)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>Deformed</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icecstType">
        <xsd:annotation>
            <xsd:documentation>ICECST is an indication of strength of the compacting of the
                ice.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>Little compacting</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>Slight compacting</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="12">
                <xsd:annotation>
                    <xsd:documentation>Slight to moderate compacting</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="20">
                <xsd:annotation>
                    <xsd:documentation>Moderate compacting</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="23">
                <xsd:annotation>
                    <xsd:documentation>Moderate to strong compacting</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="30">
                <xsd:annotation>
                    <xsd:documentation>Strong compacting</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="98">
                <xsd:annotation>
                    <xsd:documentation>Ice divergence</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined / Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="iceftyType">
        <xsd:annotation>
            <xsd:documentation>ICEFTY indicates the type of fracture, based upon
                width.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>Crack (0 to 1m wide)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>Very Small Fracture (&gt;1m to 50m wide)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="03">
                <xsd:annotation>
                    <xsd:documentation>Small Fracture (&gt;50m to 200m wide)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="04">
                <xsd:annotation>
                    <xsd:documentation>Medium Fracture (&gt;200m to 500m wide)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="05">
                <xsd:annotation>
                    <xsd:documentation>Large Fracture (&gt;500m wide)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icelstType">
        <xsd:annotation>
            <xsd:documentation>The Ice Lead Status indicates the surface nature of the
                lead.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>Open Lead</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>Frozen Lead</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icelorType">
        <xsd:annotation>
            <xsd:documentation>ICELOR indicates the predominant orientation of leads and
                fractures.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>No Leads or Fractures</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>NE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="03">
                <xsd:annotation>
                    <xsd:documentation>E</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="04">
                <xsd:annotation>
                    <xsd:documentation>SE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="05">
                <xsd:annotation>
                    <xsd:documentation>S</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="06">
                <xsd:annotation>
                    <xsd:documentation>SW</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="07">
                <xsd:annotation>
                    <xsd:documentation>W</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="08">
                <xsd:annotation>
                    <xsd:documentation>NW</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="09">
                <xsd:annotation>
                    <xsd:documentation>N</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>Variable</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icelocType">
        <xsd:annotation>
            <xsd:documentation>Ice Location Information</xsd:documentation>
            <xsd:documentation>Definition: ICELOC indicates whether the break is at a specific
                location, or whether there is a presence in the area.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>Specific Location</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>Presence in Area</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icebszType">
        <xsd:annotation>
            <xsd:documentation>Iceberg Size</xsd:documentation>
            <xsd:documentation>Definition: The "Iceberg Size" categorizes the size of an
                iceberg.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>Growler (&lt;1m asl)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>Bergy Bit (1-&lt;5m asl; 5-&lt;15m
                        length)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="03">
                <xsd:annotation>
                    <xsd:documentation>Small Iceberg (5-15m asl; 15-60m length)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="04">
                <xsd:annotation>
                    <xsd:documentation>Medium Iceberg (16-45m asl; 61-120m
                        length)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="05">
                <xsd:annotation>
                    <xsd:documentation>Large Iceberg (46-75m asl; 121-200m
                        length)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="06">
                <xsd:annotation>
                    <xsd:documentation>Very Large Iceberg (&gt;75m asl; &gt;200m
                        length)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="07">
                <xsd:annotation>
                    <xsd:documentation>Ice Island Fragment</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="08">
                <xsd:annotation>
                    <xsd:documentation>Ice Island (in the Northern Hemisphere) or Very Large Tabular
                        Berg (in the Southern Ocean)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="09">
                <xsd:annotation>
                    <xsd:documentation>Radar Target</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="iceddrType">
        <xsd:annotation>
            <xsd:documentation>Ice Drift Direction</xsd:documentation>
            <xsd:documentation>Definition: ICEDDR indicates the direction in which an ice mass is
                drifting.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>No Ice Motion</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>Ice Drift to NE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="03">
                <xsd:annotation>
                    <xsd:documentation>Ice Drift to E</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="04">
                <xsd:annotation>
                    <xsd:documentation>Ice Drift to SE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="05">
                <xsd:annotation>
                    <xsd:documentation>Ice Drift to S</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="06">
                <xsd:annotation>
                    <xsd:documentation>Ice Drift to SW</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="07">
                <xsd:annotation>
                    <xsd:documentation>Ice Drift to W</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="08">
                <xsd:annotation>
                    <xsd:documentation>Ice Drift to NW</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="09">
                <xsd:annotation>
                    <xsd:documentation>Ice Drift to N</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>Variable</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icettyType">
        <xsd:annotation>
            <xsd:documentation>Ice Thickness Type</xsd:documentation>
            <xsd:documentation>Definition: ICETTY indicated whether the thickness of the ice was
                measured or estimated.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>Measured</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>Estimated</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="icescnType">
        <xsd:annotation>
            <xsd:documentation>Snow Cover Concentration</xsd:documentation>
            <xsd:documentation>Definition: ICESCN indicates the concentration (aerial coverage) of
                snow in tenths.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>1/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>2/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="03">
                <xsd:annotation>
                    <xsd:documentation>3/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="04">
                <xsd:annotation>
                    <xsd:documentation>4/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="05">
                <xsd:annotation>
                    <xsd:documentation>5/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="06">
                <xsd:annotation>
                    <xsd:documentation>6/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="07">
                <xsd:annotation>
                    <xsd:documentation>7/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="08">
                <xsd:annotation>
                    <xsd:documentation>8/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="09">
                <xsd:annotation>
                    <xsd:documentation>9/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>10/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="11">
                <xsd:annotation>
                    <xsd:documentation>&lt;1/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="12">
                <xsd:annotation>
                    <xsd:documentation>No Snow Cover</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icedosType">
        <xsd:annotation>
            <xsd:documentation>Direction of Sastrugi</xsd:documentation>
            <xsd:documentation>Definition: ICEDOS indicates the bearing of a sastrugi. Sastrugi are
                sharp, irregular ridges formed on a snow surface by wind erosion and deposition. On
                mobile floating ice the ridges are parallel to the direction of the wind at the time
                they were formed.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>No Sastrugi</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="02">
                <xsd:annotation>
                    <xsd:documentation>NE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="03">
                <xsd:annotation>
                    <xsd:documentation>E</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="04">
                <xsd:annotation>
                    <xsd:documentation>SE</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="05">
                <xsd:annotation>
                    <xsd:documentation>S</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="06">
                <xsd:annotation>
                    <xsd:documentation>SW</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="07">
                <xsd:annotation>
                    <xsd:documentation>W</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="08">
                <xsd:annotation>
                    <xsd:documentation>NW</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="09">
                <xsd:annotation>
                    <xsd:documentation>N</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>Variable</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation>Undetermined/Unknown</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="icercnType">
        <xsd:annotation>
            <xsd:documentation>Ice Ridge Concentration</xsd:documentation>
            <xsd:documentation>Definition: ICERCN describes the concentration of hummocked ice in an
                ice area. Up to three values may be given in the list to correspond to the partial
                concentrations in ICEAPC.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:int">
            <xsd:enumeration value="01">
                <xsd:annotation>
                    <xsd:documentation>0/10 – 1/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation>1/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="12">
                <xsd:annotation>
                    <xsd:documentation>1/10 – 2/10</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="20">
                <xsd:annotation>
                    <xsd:documentation>2/10</xsd:documentation>
