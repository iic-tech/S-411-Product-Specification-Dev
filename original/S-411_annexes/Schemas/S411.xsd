<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
            xmlns:ice="http://www.jcomm.info/ice"
            xmlns:gml="http://www.opengis.net/gml/3.2" targetNamespace="http://www.jcomm.info/ice"
    elementFormDefault="qualified">
    <!--<xsd:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="S100_gmlProfile/S100_gmlProfile.xsd"/>-->
    <xsd:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="S411_Geometry.xsd"></xsd:import>

    <!-- Elements -->
    <xsd:element name="IceDataSet" type="ice:iceDataSetType"></xsd:element>


    <!-- Ice Feature Types (Objects) -->


    <xsd:complexType name="iceDataSetType">
        <xsd:annotation>
            <xsd:documentation>Data Set type for ice data</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="IceFeatureMember" type="ice:iceFeatureMemberType" maxOccurs="unbounded" minOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="iceFeatureMemberType">
        <xsd:annotation>
            <xsd:documentation>Ice Feature Member Type for Ice Features (IHO S-411)</xsd:documentation>
        </xsd:annotation>
        <xsd:choice>
            <!-- Area Elements -->
            <xsd:element name="seaice" type="ice:seaiceType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="lacice" type="ice:laciceType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="brgare" type="ice:brgareType" maxOccurs="1" minOccurs="1"/>
            <!-- LineString Elements -->
            <xsd:element name="icelne" type="ice:icelneType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="brglne" type="ice:brglneType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="opnlne" type="ice:opnlneType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="lkilne" type="ice:lkilneType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="i_ridg" type="ice:i_ridgType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="i_lead" type="ice:i_leadType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="i_fral" type="ice:i_fralType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="i_crac" type="ice:i_cracType" maxOccurs="1" minOccurs="1"/>
            <!-- Point Elements -->
            <xsd:element name="icecom" type="ice:icecomType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="icelea" type="ice:iceleaType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="icebrg" type="ice:icebrgType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="flobrg" type="ice:flobrgType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="icethk" type="ice:icethkType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="iceshr" type="ice:iceshrType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="icediv" type="ice:icedivType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="icerdg" type="ice:icerdgType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="icekel" type="ice:icekelType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="icedft" type="ice:icedftType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="icefra" type="ice:icefraType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="icerft" type="ice:icerftType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="jmdbrr" type="ice:jmdbrrType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="stgmlt" type="ice:stgmltType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="snwcvr" type="ice:snwcvrType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="strptc" type="ice:strptcType" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="i_grhm" type="ice:i_grhmType" maxOccurs="1" minOccurs="1"/>
        </xsd:choice>
    </xsd:complexType>


    <!-- 
        Abstract base ice feature type for defining of global attributes and elements for all feature types
        in this case is gml:id defined which is required for all ice features.
      
        All other ice feature types are extensions of this base type 
    -->

    <xsd:complexType name="iceFeatureType" abstract="true">
        <xsd:annotation>
            <xsd:documentation>
                Abstract base ice feature type for defining of global attributes and elements for all feature types
                in this case is gml:id defined which is required for all ice features.
                All other ice feature types are extensions of this base type.
            </xsd:documentation>
        </xsd:annotation>
       <!-- <xsd:choice>
            <xsd:element ref="gml:Point"/>
            <xsd:element ref="gml:Polygon"/>
            <xsd:element ref="gml:LineString"/>
        </xsd:choice>-->
        <xsd:attribute ref="gml:id" use="required"/>    
    </xsd:complexType>


    <xsd:complexType name="seaiceType">
        <xsd:annotation>
            <xsd:documentation>Sea Ice</xsd:documentation>
            <xsd:documentation>Definition: Sea Ice is an area at sea that is covered, in whole or in
                part, with ice.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="iceact" type="ice:iceactType" minOccurs="0"/>
                    <xsd:element name="iceapc" type="ice:iceapcType" minOccurs="0"/>
                    <xsd:element name="icesod" type="ice:icesodType" minOccurs="0"/>
                    <xsd:element name="iceflz" type="ice:iceflzType" minOccurs="0"/>
                    <xsd:element name="icespc" type="ice:icespcType" minOccurs="0"/>
                    <xsd:element name="icelvl" type="ice:icelvlType" minOccurs="0"/>
                    <xsd:element name="icecst" type="ice:icecstType" minOccurs="0"/>
                    <xsd:element name="icefty" type="ice:iceftyType" minOccurs="0"/>
                    <xsd:element name="icedsp" type="xsd:float" minOccurs="0"/>
                    <xsd:element name="iceddr" type="ice:iceddrType" minOccurs="0"/>
                    <xsd:element name="icercn" type="ice:icercnType" minOccurs="0"/>
                    <xsd:element name="icerfq" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icermh" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icerxh" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icerdv" type="ice:icerdvType" minOccurs="0"/>
                    <xsd:element name="icekcn" type="ice:icekcnType" minOccurs="0"/>
                    <xsd:element name="icekfq" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icekmd" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icekxd" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icefcn" type="ice:icefcnType" minOccurs="0"/>
                    <xsd:element name="icetck" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icemax" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icemin" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icetty" type="ice:icettyType" minOccurs="0"/>
                    <xsd:element name="icemlt" type="ice:icemltType" minOccurs="0"/>
                    <xsd:element name="icescn" type="ice:icescnType" minOccurs="0"/>
                    <xsd:element name="icesct" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icedos" type="ice:icedosType" minOccurs="0"/>
                    <xsd:element name="icelst" type="ice:icelstType" minOccurs="0"/>
                    <xsd:element name="icelfq" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icelor" type="ice:icelorType" minOccurs="0"/>
                    <xsd:element name="icelwd" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="ia_sfa" type="ice:ia_sfaType" minOccurs="0"/>
                    <xsd:element name="ia_sfb" type="ice:ia_sfbType" minOccurs="0"/>
                    <xsd:element name="ia_sfc" type="ice:ia_sfcType" minOccurs="0"/>
                    <xsd:element name="ia_ffa" type="ice:ia_ffaType" minOccurs="0"/>
                    <xsd:element name="ia_ffb" type="ice:ia_ffbType" minOccurs="0"/>
                    <xsd:element name="ia_ffc" type="ice:ia_ffcType" minOccurs="0"/>
                    <xsd:element name="ia_sng" type="ice:ia_sngType" minOccurs="0"/>
                    <xsd:element name="ia_mlt" type="ice:ia_mltType" minOccurs="0"/>
                    <xsd:element name="ia_plg" type="ice:ia_plgType" minOccurs="0"/>
                    <xsd:element name="ia_hlg" type="ice:ia_hlgType" minOccurs="0"/>
                    <xsd:element name="ia_dug" type="ice:ia_dugType" minOccurs="0"/>
                    <xsd:element ref="gml:Polygon" minOccurs="1"></xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="laciceType">
        <xsd:annotation>
            <xsd:documentation>Lake Ice</xsd:documentation>
            <xsd:documentation>Definition: Lake Ice is an area on a lake that is covered, in whole
                or in part, with ice.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="iceact" type="ice:iceactType" minOccurs="0"/>
                    <xsd:element name="iceapc" type="ice:iceapcType" minOccurs="0"/>
                    <xsd:element name="icelso" type="ice:icelsoType" minOccurs="0"/>
                    <xsd:element name="iceflz" type="ice:iceflzType" minOccurs="0"/>
                    <xsd:element name="icespc" type="ice:icespcType" minOccurs="0"/>
                    <xsd:element name="icelvl" type="ice:icelvlType" minOccurs="0"/>
                    <xsd:element name="icecst" type="ice:icecstType" minOccurs="0"/>
                    <xsd:element name="icefty" type="ice:iceftyType" minOccurs="0"/>
                    <xsd:element name="icedsp" type="xsd:float" minOccurs="0"/>
                    <xsd:element name="iceddr" type="ice:iceddrType" minOccurs="0"/>
                    <xsd:element name="icercn" type="ice:icercnType" minOccurs="0"/>
                    <xsd:element name="icerfq" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icermh" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icerxh" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icerdv" type="ice:icerdvType" minOccurs="0"/>
                    <xsd:element name="icekcn" type="ice:icekcnType" minOccurs="0"/>
                    <xsd:element name="icekfq" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icekmd" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icekxd" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icefcn" type="ice:icefcnType" minOccurs="0"/>
                    <xsd:element name="icetck" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icemax" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icemin" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icetty" type="ice:icettyType" minOccurs="0"/>
                    <xsd:element name="icemlt" type="ice:icemltType" minOccurs="0"/>
                    <xsd:element name="icescn" type="ice:icescnType" minOccurs="0"/>
                    <xsd:element name="icesct" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icedos" type="ice:icedosType" minOccurs="0"/>
                    <xsd:element name="icelst" type="ice:icelstType" minOccurs="0"/>
                    <xsd:element name="icelfq" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icelor" type="ice:icelorType" minOccurs="0"/>
                    <xsd:element name="icelwd" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element ref="gml:Polygon"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="brgareType">
        <xsd:annotation>
            <xsd:documentation/>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icebnm" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="icebsz" type="ice:icebszType" minOccurs="0"/>
                    <xsd:element name="ia_bcn" type="ice:ia_bcnType" minOccurs="0"/>
                    <xsd:element name="ia_bfm" type="ice:ia_bfmType" minOccurs="0"/>
                    <xsd:element name="ia_buh" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element ref="gml:Polygon"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="icelneType">
        <xsd:annotation>
            <xsd:documentation>Ice Edge</xsd:documentation>
            <xsd:documentation>Definition: The demarcation at any given time between the open sea
                and sea ice of any kind and in any concentration, whether fast or
                drifting.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element ref="gml:LineString"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="brglneType">
        <xsd:annotation>
            <xsd:documentation>Iceberg Limit</xsd:documentation>
            <xsd:documentation>Definition: Limit of all known Icebergs</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element ref="gml:LineString"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="opnlneType">
        <xsd:annotation>
            <xsd:documentation>Limit of Open Water</xsd:documentation>
            <xsd:documentation>Definition: The demarcation at any given time between sea ice and
                freely navigable water, in which sea ice is present in concentrations less than
                1/10.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element ref="gml:LineString"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="lkilneType">
        <xsd:annotation>
            <xsd:documentation>Limit of All Known Ice</xsd:documentation>
            <xsd:documentation>Definition: The limit of all known ice, including both sea ice of any
                kind and icebergs.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element ref="gml:LineString"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="i_ridgType">
        <xsd:annotation>
            <xsd:documentation/>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icerdv" type="ice:icerdvType" minOccurs="0"/>
                    <xsd:element name="icermh" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icerxh" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element ref="gml:LineString"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="i_leadType">
        <xsd:annotation>
            <xsd:documentation>Line of Ice Lead</xsd:documentation>
            <xsd:documentation>Definition: Line of Ice Lead identifies any passage-way(s) through
                ice which is (are) navigable by surface vessels.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icesod" type="ice:icesodType" minOccurs="0"/>
                    <xsd:element name="ia_obn" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icedvw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="ia_dmw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="ia_dxw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element ref="gml:LineString"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="i_fralType">
        <xsd:annotation>
            <xsd:documentation>Line of Ice Fracture</xsd:documentation>
            <xsd:documentation>Definition: Any break or rupture through the ice cover, or through
                the single floe, resulting from deformation processes. Length may vary from a few
                meters to a few kilometers.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icesod" type="ice:icesodType" minOccurs="0"/>
                    <xsd:element name="ia_obn" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icedvw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="ia_dmw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="ia_dxw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element ref="gml:LineString"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="i_cracType">
        <xsd:annotation>
            <xsd:documentation>Line of Ice Crack</xsd:documentation>
            <xsd:documentation>Definition: Line of Ice Crack identifies any ice breakup, but no
                passage-way(s) for surface vessels.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icesod" type="ice:icesodType" minOccurs="0"/>
                    <xsd:element name="ia_obn" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icedvw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="ia_dmw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="ia_dxw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element ref="gml:LineString"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>


    <xsd:complexType name="icecomType">
        <xsd:annotation>
            <xsd:documentation>Ice Compacting</xsd:documentation>
            <xsd:documentation>Definition: Pieces of ice are said to be compacting when they are
                subjected to a converging motion, which increases ice concentration and/or produces
                stresses which may result in ice deformation.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icecst" type="ice:icecstType" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="iceleaType">
        <xsd:annotation>
            <xsd:documentation>Ice Lead</xsd:documentation>
            <xsd:documentation>Definition: Ice Lead identifies any fracture(s) or passage-way(s)
                through ice which is (are) navigable by surface vessels.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="iceloc" type="ice:icelocType" minOccurs="0"/>
                    <xsd:element name="icelst" type="ice:icelstType" minOccurs="0"/>
                    <xsd:element name="icelwd" type="xsd:nonNegativeInteger"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="icebrgType">
        <xsd:annotation>
            <xsd:documentation>Iceberg</xsd:documentation>
            <xsd:documentation>Definition: An Iceberg is a massive piece of ice, greatly varying in
                shape and showing more than 5 meters above the sea surface which has broken away
                from a glacier, and which may be afloat or grounded. This Object Class also includes
                smaller forms of glacial ice, known as “Bergy Bits” and “Growlers”, which are
                defined by their size Attribute. ICEBRG can indicate the location of a single
                iceberg or, if the attribute IA_OBN is specified and is greater than 1, multiple
                icebergs in the vicinity of a point.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icebsz" type="ice:icebszType" minOccurs="0"/>
                    <xsd:element name="icedsp" type="xsd:float" minOccurs="0"/>
                    <xsd:element name="iceddr" type="ice:iceddrType" minOccurs="0"/>
                    <xsd:element name="ia_obn" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="ia_bfm" type="ice:ia_bfmType" minOccurs="0"/>
                    <xsd:element name="ia_buh" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="flobrgType">
        <xsd:annotation>
            <xsd:documentation>Floeberg</xsd:documentation>
            <xsd:documentation>Definition: A Floeberg is a massive piece of sea ice composed of a
                hummock or a group of hummocks, frozen together and separated from any ice
                surroundings. They typically protrude up to 5 meters above the sea
                surface.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icedsp" type="xsd:float" minOccurs="0"/>
                    <xsd:element name="iceddr" type="ice:iceddrType" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="icethkType">
        <xsd:annotation>
            <xsd:documentation>Ice Thickness</xsd:documentation>
            <xsd:documentation>Definition: Ice Thickness provides a measure or estimate of ice
                thickness.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icetck" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icemax" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icemin" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icetty" type="ice:icettyType" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="iceshrType">
        <xsd:annotation>
            <xsd:documentation>Ice Shear</xsd:documentation>
            <xsd:documentation>Definition: An area of drift ice is subject to shear when the ice
                motion varies significantly in the direction normal to the motion, subjecting the
                ice to rotational forces</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="icedivType">
        <xsd:annotation>
            <xsd:documentation>Ice Divergence</xsd:documentation>
            <xsd:documentation>Definition: Ice fields or floes in an area are subject to diverging
                or dispersive motion, thus reducing ice concentration and/or relieving stresses in
                the ice.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="icerdgType">
        <xsd:annotation>
            <xsd:documentation>Ice Ridge/Hummock</xsd:documentation>
            <xsd:documentation>Definition: An Ice Ridge is a line or wall of broken ice forced up by
                pressure. A Hummock is a hillock of broken ice which has been forced upward by
                pressure.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icercn" type="ice:icercnType" minOccurs="0"/>
                    <xsd:element name="icerfq" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icermh" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icerxh" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icerdv" type="ice:icerdvType" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="icekelType">
        <xsd:annotation>
            <xsd:documentation>Ice Keel/Bummock</xsd:documentation>
            <xsd:documentation>Definition: From a submariner’s point of view, a Keel is a downward
                projecting ridge on the underside of the ice canopy - the counterpart of a Ridge. A
                Bummock is the counterpart of a hummock on the underside of the ice
                canopy.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icekcn" type="ice:icekcnType" minOccurs="0"/>
                    <xsd:element name="icekfq" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icekmd" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icekxd" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="icedftType">
        <xsd:annotation>
            <xsd:documentation>Ice Drift</xsd:documentation>
            <xsd:documentation>Definition: Motion of an ice field or floe as a result of forces such
                as wind and currents.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icedsp" type="xsd:float" minOccurs="0"/>
                    <xsd:element name="iceddr" type="ice:iceddrType" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="icefraType">
        <xsd:annotation>
            <xsd:documentation>Ice Fracture</xsd:documentation>
            <xsd:documentation>Definition: Any break or rupture through the ice pack, or a single
                floe, resulting from deformation processes. Length may vary from a few metres to
                many kilometres.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icefty" type="ice:iceftyType" minOccurs="0"/>
                    <xsd:element name="iceloc" type="ice:icelocType" minOccurs="0"/>
                    <xsd:element name="ia_obn" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icesod" type="ice:icesodType" minOccurs="0"/>
                    <xsd:element name="icedvw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="ia_dmw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="ia_dxw" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="icerftType">
        <xsd:annotation>
            <xsd:documentation>Ice Rafting</xsd:documentation>
            <xsd:documentation>Definition: Pressure processes whereby one piece of ice overrides
                another.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icefcn" type="ice:icefcnType" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="jmdbrrType">
        <xsd:annotation>
            <xsd:documentation>Jammed Brash Barrier</xsd:documentation>
            <xsd:documentation>Definition: A strip or narrow belt of new, young or brash ice
                (usually 100-500 metres wide) formed at the edge of either drift or fast
                ice.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="stgmltType">
        <xsd:annotation>
            <xsd:documentation>Stage of Melt</xsd:documentation>
            <xsd:documentation>Definition: A description of the stage of melt of the ice; i.e.
                whether it has formed puddles on the surface and whether these have
                frozen.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icemlt" type="ice:icemltType" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="snwcvrType">
        <xsd:annotation>
            <xsd:documentation>Snow Cover</xsd:documentation>
            <xsd:documentation>Definition: A description of the amount of snow covering the
                ice.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icescn" type="ice:icescnType" minOccurs="0"/>
                    <xsd:element name="icesct" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element name="icedos" type="ice:icedosType" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="strptcType">
        <xsd:annotation>
            <xsd:documentation>Strips and Patches</xsd:documentation>
            <xsd:documentation>Definition: A strip is a long narrow area of floating ice, about 1
                kilometre or less in width, usually composed of small fragments detached from the
                main mass of ice, and run together under the influence of wind, swell or current. If
                the area of ice becomes more rounded in shape, it is referred to as a
                patch.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="icespc" type="ice:icespcType" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="i_grhmType">
        <xsd:annotation>
            <xsd:documentation/>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ice:iceFeatureType">
                <xsd:sequence>
                    <xsd:element name="ia_buh" type="xsd:nonNegativeInteger" minOccurs="0"/>
                    <xsd:element ref="gml:Point"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

