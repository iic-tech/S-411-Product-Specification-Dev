    <xs:complexType name="IceRidgeHummockType" abstract="false">
        <xs:annotation>
            <xs:documentation>An Ice Ridge is a line or wall of broken ice forced up by pressure. A Hummock is a hillock of broken ice which has been forced upward by pressure.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <!-- attributes -->
<!--0-->  <xs:element name="iceRidgeConcentration" type="iceRidgeConcentrationType" minOccurs="0" maxOccurs="1"/>
<!--1-->  <xs:element name="iceRidgeFrequency" type="iceRidgeFrequencyType" minOccurs="0" maxOccurs="3"/>
<!--2-->  <xs:element name="iceRidgeMeanHeight" type="iceRidgeMeanHeightType" minOccurs="0" maxOccurs="1"/>
<!--3-->  <xs:element name="iceRidgeMaximumHeight" type="iceRidgeMaximumHeightType" minOccurs="0" maxOccurs="1"/>
<!--4-->  <xs:element name="iceRidgeClassification" type="iceRidgeClassificationType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element ref="S100:pointProperty"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
 </xs:sequence>
            </xs:extension>        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="IceKeelBummockType" abstract="false">
        <xs:annotation>
            <xs:documentation>From a submariner’s point of view, a Keel is a downward projecting ridge on the underside of the ice canopy - the counterpart of a Ridge. A Bummock is the counterpart of a hummock on the underside of the ice canopy.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <!-- attributes -->
<!--0-->  <xs:element name="iceKeelConcentration" type="iceKeelConcentrationType" minOccurs="0" maxOccurs="1"/>
<!--1-->  <xs:element name="iceKeelFrequency" type="iceKeelFrequencyType" minOccurs="0" maxOccurs="1"/>
<!--2-->  <xs:element name="iceKeelMeanDepth" type="iceKeelMeanDepthType" minOccurs="0" maxOccurs="1"/>
<!--3-->  <xs:element name="iceKeelMaximumDepth" type="iceKeelMaximumDepthType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element ref="S100:pointProperty"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
 </xs:sequence>
            </xs:extension>        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="IceDriftType" abstract="false">
        <xs:annotation>
            <xs:documentation>Motion of an ice field or floe as a result of forces such as wind and currents.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <!-- attributes -->
<!--0-->  <xs:element name="iceDriftSpeed" type="iceDriftSpeedType" minOccurs="0" maxOccurs="1"/>
<!--1-->  <xs:element name="iceDriftDirection" type="iceDriftDirectionType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element ref="S100:pointProperty"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
 </xs:sequence>
            </xs:extension>        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="IceFractureType" abstract="false">
        <xs:annotation>
            <xs:documentation>Any break or rupture through the ice pack, or a single floe, resulting from deformation processes. Length may vary from a few metres to many kilometres.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <!-- attributes -->
<!--0-->  <xs:element name="iceFractureType" type="iceFractureTypeType" minOccurs="0" maxOccurs="1"/>
<!--1-->  <xs:element name="iceLocationInformation" type="iceLocationInformationType" minOccurs="0" maxOccurs="1"/>
<!--2-->  <xs:element name="numberofIceObjects" type="numberofIceObjectsType" minOccurs="0" maxOccurs="1"/>
<!--3-->  <xs:element name="iceStageofDevelopment" type="iceStageofDevelopmentType" minOccurs="0" maxOccurs="1"/>
<!--4-->  <xs:element name="averageWidthofIceLead" type="averageWidthofIceLeadType" minOccurs="0" maxOccurs="5"/>
<!--5-->  <xs:element name="minimumWidthofIceLead" type="minimumWidthofIceLeadType" minOccurs="0" maxOccurs="1"/>
<!--6-->  <xs:element name="maximumWidthofIceLead" type="maximumWidthofIceLeadType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element ref="S100:pointProperty"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
 </xs:sequence>
            </xs:extension>        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="IceRaftingType" abstract="false">
        <xs:annotation>
            <xs:documentation>Pressure processes whereby one piece of ice overrides another.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <!-- attributes -->
<!--0-->  <xs:element name="iceRaftingConcentration" type="iceRaftingConcentrationType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element ref="S100:pointProperty"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
 </xs:sequence>
            </xs:extension>        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="JammedBrashBarrierType" abstract="false">
        <xs:annotation>
            <xs:documentation>A strip or narrow belt of new, young or brash ice (usually 100-500 metres wide) formed at the edge of either drift or fast ice.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <!-- attributes -->
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element ref="S100:pointProperty"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
 </xs:sequence>
            </xs:extension>        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="StageOfMeltType" abstract="false">
        <xs:annotation>
            <xs:documentation>A description of the stage of melt of the ice; i.e. whether it has formed puddles on the surface and whether these have frozen.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <!-- attributes -->
<!--0-->  <xs:element name="meltStage" type="meltStageType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element ref="S100:pointProperty"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
 </xs:sequence>
            </xs:extension>        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SnowCoverType" abstract="false">
        <xs:annotation>
            <xs:documentation>A description of the amount of snow covering the ice.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <!-- attributes -->
<!--0-->  <xs:element name="snowCoverConcentration" type="snowCoverConcentrationType" minOccurs="0" maxOccurs="1"/>
<!--1-->  <xs:element name="snowDepth" type="snowDepthType" minOccurs="0" maxOccurs="1"/>
<!--2-->  <xs:element name="directionofSastrugi" type="directionofSastrugiType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element ref="S100:pointProperty"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
 </xs:sequence>
            </xs:extension>        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="StripsAndPatchesType" abstract="false">
        <xs:annotation>
            <xs:documentation>A strip is a long narrow area of floating ice, about 1 kilometre or less in width, usually composed of small fragments detached from the main mass of ice, and run together under the influence of wind, swell or current. If the area of ice becomes more rounded in shape, it is referred to as a patch.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <!-- attributes -->
<!--0-->  <xs:element name="concentrationofStripsandPatches" type="concentrationofStripsandPatchesType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element ref="S100:pointProperty"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
 </xs:sequence>
            </xs:extension>        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="GroundedHummockType" abstract="false">
        <xs:annotation>
            <xs:documentation>Grounded Hummock identifies a hummock formation which is stranded.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <!-- attributes -->
<!--0-->  <xs:element name="maximumHeightofAboveWaterPart" type="maximumHeightofAboveWaterPartType" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:choice>
                                <xs:element ref="S100:pointProperty"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
 </xs:sequence>
            </xs:extension>        </xs:complexContent>
    </xs:complexType>
    <xs:element name="DataCoverage" type="DataCoverageType"/>
    <xs:element name="SeaIce" type="SeaIceType"/>
    <xs:element name="LakeIce" type="LakeIceType"/>
    <xs:element name="IcebergArea" type="IcebergAreaType"/>
    <xs:element name="IceEdge" type="IceEdgeType"/>
    <xs:element name="IcebergLimit" type="IcebergLimitType"/>
    <xs:element name="LimitOfOpenWater" type="LimitOfOpenWaterType"/>
    <xs:element name="LimitOfAllKnownIce" type="LimitOfAllKnownIceType"/>
    <xs:element name="LineOfIceRidge" type="LineOfIceRidgeType"/>
    <xs:element name="LineOfIceLead" type="LineOfIceLeadType"/>
    <xs:element name="LineOfIceFracture" type="LineOfIceFractureType"/>
    <xs:element name="LineOfIceCrack" type="LineOfIceCrackType"/>
    <xs:element name="IceCompacting" type="IceCompactingType"/>
    <xs:element name="IceLead" type="IceLeadType"/>
    <xs:element name="Iceberg" type="IcebergType"/>
    <xs:element name="Floeberg" type="FloebergType"/>
    <xs:element name="IceThickness" type="IceThicknessType"/>
    <xs:element name="IceShear" type="IceShearType"/>
    <xs:element name="IceDivergence" type="IceDivergenceType"/>
    <xs:element name="IceRidgeHummock" type="IceRidgeHummockType"/>
    <xs:element name="IceKeelBummock" type="IceKeelBummockType"/>
    <xs:element name="IceDrift" type="IceDriftType"/>
    <xs:element name="IceFracture" type="IceFractureType"/>
    <xs:element name="IceRafting" type="IceRaftingType"/>
    <xs:element name="JammedBrashBarrier" type="JammedBrashBarrierType"/>
    <xs:element name="StageOfMelt" type="StageOfMeltType"/>
    <xs:element name="SnowCover" type="SnowCoverType"/>
    <xs:element name="StripsAndPatches" type="StripsAndPatchesType"/>
    <xs:element name="GroundedHummock" type="GroundedHummockType"/>
    <xs:group name="MemberObjects">
        <xs:annotation>
            <xs:documentation>group of informtion, feature and meta objects, all GML members</xs:documentation>
        </xs:annotation>
        <xs:choice>
<xs:element ref="DataCoverage"/><xs:element ref="SeaIce"/><xs:element ref="LakeIce"/><xs:element ref="IcebergArea"/><xs:element ref="IceEdge"/><xs:element ref="IcebergLimit"/><xs:element ref="LimitOfOpenWater"/><xs:element ref="LimitOfAllKnownIce"/><xs:element ref="LineOfIceRidge"/><xs:element ref="LineOfIceLead"/><xs:element ref="LineOfIceFracture"/><xs:element ref="LineOfIceCrack"/><xs:element ref="IceCompacting"/><xs:element ref="IceLead"/><xs:element ref="Iceberg"/><xs:element ref="Floeberg"/><xs:element ref="IceThickness"/><xs:element ref="IceShear"/><xs:element ref="IceDivergence"/><xs:element ref="IceRidgeHummock"/><xs:element ref="IceKeelBummock"/><xs:element ref="IceDrift"/><xs:element ref="IceFracture"/><xs:element ref="IceRafting"/><xs:element ref="JammedBrashBarrier"/><xs:element ref="StageOfMelt"/><xs:element ref="SnowCover"/><xs:element ref="StripsAndPatches"/><xs:element ref="GroundedHummock"/>        </xs:choice>
    </xs:group>
    <xs:complexType name="ThisDatasetType">
        <xs:complexContent>
            <xs:extension base="S100:DatasetType">
                <xs:sequence>
                    <xs:element name="members">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="gml:AbstractFeatureMemberType">
                                    <xs:sequence>
                                        <xs:group ref="MemberObjects" minOccurs="0" maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="Dataset" type="ThisDatasetType"/>
</xs:schema>
